algoritmo "semnome"
// Função :
// Autor :
// Data : 18/09/2020
// Seção de Declarações 
var
variavelA, variavelB, variavelAjuda: inteiro
inicio

    variavelA := 10
    variavelB := 20
    variavelAjuda := variavelA

    variavelA := variavelB
    variavelB := variavelAjuda
    
    escreval (variavelA)
    escreval (variavelB)

fimalgoritmo
